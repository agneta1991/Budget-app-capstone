<div class = 'main_div'>
<nav>
<%= link_to groups_path, class: 'back_btn' do %>
<img class='back_btn'src="https://img.icons8.com/ios/50/ffffff/back--v1.png" alt="back--v1"/>
<%end%>
<p>LOG IN</p>
  <%= link_to "SIGN UP", new_registration_path(resource_name) %><br />
</nav>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: 'example@example.com'%>
  </div>

  <div class="field">
    <%= f.password_field :password, autocomplete: "current-password", placeholder: 'Password'%>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="field custom-field">
      <%= f.label :remember_me, class: 'remember' %>
      <%= f.check_box :remember_me, class: 'checkbox'%>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "Log in", class: 'registration_btn'%>
  </div>
<% end %>

  <%= link_to "Forgot your password?", new_password_path(resource_name), class: 'forgot' %><br />
</div>
</div>